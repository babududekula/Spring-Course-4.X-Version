	<property name="jakarta.persistence.jdbc.password" value="babu"/>
	<property name="hibernate.dialect" value="org.hibernate.dialect.Oracle10gDialect"/>
	<property name="hibernate.show_sql" value="true"/>
	</properties>
</persistence-unit>
</persistence>