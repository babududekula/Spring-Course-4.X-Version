<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
	<class name="com.pack.pojo.Bank" table="bank">
		<id name="bname" column="BNAME"/>
		<property name="bid" column="BID"/>
		<property name="baccount" column="BACCOUNT"/>
		<property name="bbalance" column="BBALANCE"/>
		<property name="bbranch" column="BBRANCH"/>
	</class>
</hibernate-mapping>