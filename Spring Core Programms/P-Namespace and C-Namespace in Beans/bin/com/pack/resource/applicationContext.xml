<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:c="http://www.springframework.org/schema/c"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
        
 <bean id="course" class="com.pack.beans.Course" p:cname="Java" p:ctype="3 months" p:std-ref="std"/>
 <bean id="std" class="com.pack.beans.Student" p:sname="Babu" p:sage="25"/>   
 <bean id="customer" class="com.pack.beans.Customer" c:cname="Dudekula Babu" c:cage="26" c:csal="56800" c:employee-ref="employee"/>
 <bean id="employee" class="com.pack.beans.Employee" c:_0="Dudekula" c:_1="25" c:_2="45000" />     
 </beans>       