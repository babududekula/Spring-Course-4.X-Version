
public class Demo 
{
	int add(int a , int b)
	{
		int c = a+b;
		return c;
	}
	
	public static int add1(int a , int b)
	{
		int c = a + b;
		return c ;
	}
	
	public static void main(String[] args) 
	{
		Demo Babu = new Demo();
		int babu = Babu.add(20, 30);
		System.out.println(babu);
		
		int babu1 = new Demo().add(20, 40);
		System.out.println(babu1);
		
		System.out.println(Babu.add(200, 100));
		
	}

}
