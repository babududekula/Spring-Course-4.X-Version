import java.util.Scanner;

public class ClassA {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int no ;
		Scanner s = new Scanner("System.in()");
		no = s.nextInt();
		int temp = no;
		int rem , rev =0;
		while(temp !=0)
		{
			rem = no % 10;
			rev = ( rev * 10 ) + rem;
			no = no / 10;
			
		}
		if(temp == rev)
		{
			System.out.println("palindrome");
		}
		else
		{
			System.out.println("not palindrome");
		}
	}

}
