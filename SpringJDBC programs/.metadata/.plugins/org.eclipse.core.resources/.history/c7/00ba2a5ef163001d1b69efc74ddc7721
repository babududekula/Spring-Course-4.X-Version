import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.ResultSet;
import java.sql.Statement;

public class ClassA 
{

	public static void main(String[] args) 
	{
		try {
			Class.forName("oracle.jdbc.OracleDriver");
			Connection con = DriverManager.getConnection("jdbc:oracle:thin:@localhost:1521:xe", "system", "bhavani");
			Statement st = con.createStatement();
			
			st.execute("Create table Babu(no number(4) primary key , name varchar2(20) , esal float(5,2)");
			System.out.println("Suucess");
		} 
		catch (Exception e) 
		{
			e.printStackTrace();
		}
	}

}
